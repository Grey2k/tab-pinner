{
  "name": "TAB Pinner",
  "version": "1.0",
  "description": "Your tabs always upfront of you!",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Restore Pinned Tabs",
    "default_icon": {
      "16": "images/tab-icon.png",
      "32": "images/tab-icon.png",
      "48": "images/tab-icon.png",
      "128": "images/tab-icon.png"
    }
  },
  "icons": {
    "16": "images/tab-icon.png",
    "32": "images/tab-icon.png",
    "48": "images/tab-icon.png",
    "128": "images/tab-icon.png"
  },
  "manifest_version": 2
}